type Query {
    bookById(id: ID): Book
    employeeById(id: ID): Employee
}

type Book {
    id: ID
    name: String
    pageCount: Int
    author: Author
}

type Author {
    id: ID
    firstName: String
    lastName: String
}

type Employee {
    employeeID: ID
    firstName: String
    lastName: String
    email: String
    phoneNumber: String
    hireDate: String
    commissionPct: String
    jobId: String

}